[all:vars]
ansible_ssh_common_args='-o StrictHostKeyChecking=no'

[jenkins-agents]

[centos-agent]
centos-agent ansible_ssh_host=centos-agent 

[debian-agent]
debian-agent ansible_ssh_host=debian-agent

[ubuntu-agent]
ubuntu-agent ansible_ssh_host=ubuntu-agent

[jenkins-agents:vars]
ansible_ssh_port=22 
ansible_user=jenkins

[jenkins-agents:children]
centos-agent
debian-agent
ubuntu-agent
